{
  "name": "kio-ng2-navigation",
  "version": "0.0.8",
  "description": "Navigation components",
  "main": "./src/navigation.module",
  "module": "./src/navigation.module",
  "typings": "./src/navigation.module.ts",
  "scripts": {
    "test": "ts-mocha ./src/**/*.spec.ts",
    "pretest": "npm run build",
    "test:watch": "./scripts/watch.sh test",
    "lint": "tslint ./src/**/*.ts ./test/**/*.ts",
    "version": "npm run build",
    "postversion": "git push && git push --tags",
    "predoc": "rm -rf ./documentation",
    "doc": "compodoc -p ./tsconfig-doc.json -d ./documentation/",
    "sass:build": "./scripts/compile-scss.sh ./build",
    "transpile": "npm run ngModule:build",
    "ngModule:build": "./scripts/ngModule.sh build",
    "ngModule:copy_build": "./scripts/ngModule.sh copy_build",
    "ngModule:copy": "./scripts/sync_to.sh ../../digit-rfn",
    "fetchdeps": "echo ./scripts/sync_kio_module.sh component-routing ctn i18n",
    "build": "npm run ngModule:copy_build && npm run sass:build && npm run ngModule:build",
    "dev:sync": "./scripts/sync_to.sh ../../digit-rfn",
    "notify:build:start": "./scripts/ngModule.sh notify \"Build started\"",
    "notify:build:end": "./scripts/ngModule.sh notify done \"Build finished\"",
    "dev:sync:watch": "./scripts/watch.sh notify:build:start dev:sync notify:build:end",
    "build:dev": "NODE_ENV=development npm run build",
    "build:dev:watch": "./scripts/watch.sh notify:build:start build:dev notify:build:end",
    "build:release": "NODE_ENV=production npm run build",
    "build:release:watch": "./scripts/watch.sh build:release"
  },
